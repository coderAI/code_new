<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="vhr_program_part_tree_view">
			<field name="name">vhr.program.part.tree</field>
			<field name="model">vhr.program.part</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Question Part">					
					<field name="name" />
					<field name="name_en" />
					<field name="code"/>
					<field name="group" />
					<field name="description" />					
				</tree>
			</field>
		</record>

		<record model="ir.ui.view" id="vhr_program_part_form_view">
			<field name="name">vhr.program.part.form</field>
			<field name="model">vhr.program.part</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Question Part">
					<sheet>
						<group>
							<group>
								<field name="name" required="1" />
								<field name="code"/>								
								<field name="description"/>
							</group>
							<group>
								<field name="name_en" />
								<field name="group"/>								
								<field name="description_en" />
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>
	</data>
</openerp>
            
         