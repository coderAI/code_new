<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate='0'>
		<record model="ir.actions.act_window" id="action_vhr_rr_import_status">
			<field name="name">RR Import status</field>
			<field name="res_model">vhr.import.status</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_mode">tree,form</field>
			<field name="groups_id" eval="" />
			<field name="domain"
				eval="[('module_id.name', '=', 'vhr_recruitment')]" />
			<field name="search_view_id" ref="vhr_common.view_vhr_import_status_search" />
		</record>
		<record id="action_vhr_rr_import_status_tree" model="ir.actions.act_window.view">
			<field name="sequence">1</field>
			<field name="view_id" ref="vhr_common.view_vhr_import_status_tree" />
			<field name="act_window_id" ref="action_vhr_rr_import_status" />
			<field name="view_mode">tree</field>
		</record>
		<record id="action_vhr_rr_import_status_form" model="ir.actions.act_window.view">
			<field name="sequence">2</field>
			<field name="view_id" ref="vhr_common.view_vhr_import_status_form" />
			<field name="act_window_id" ref="action_vhr_rr_import_status" />
			<field name="view_mode">form</field>
		</record>

		<!-- Import key student -->
		<record id="view_vhr_import_status_key_student" model="ir.ui.view">
			<field name="name">vhr.import.status.key.student.form</field>
			<field name="model">vhr.import.status</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Import Key Student">
					<div class="oe_title">
						<h1>
							<label string="Import Key Student" />
						</h1>
					</div>
					<br />
					<group>
						<field name="db_datas" filename="name_data" />
						<field name="name_data" readonly="1" options="{'save_readonly' : True}" />
					</group>
					<div>
						<![CDATA[
							<a href="/vhr_recruitment/static/template/key_student_template.xlsx">Key student template</a>
						]]>
					</div>
					<footer>
						<button name="import_excel_thread" string="Import"
							context="{'function': 'thread_import_key_student',
										  'model': 'vhr.rr.key.student',
										  'module':'module_vhr_recruitment'}"
							type="object" icon="gtk-apply"
							default_focus="1" />
						or
						<button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="action_vhr_import_status_key_student" model="ir.actions.act_window">
			<field name="name">Import key student</field>
			<field name="res_model">vhr.import.status</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>
		<record id="action_vhr_import_status_key_form" model="ir.actions.act_window.view">
			<field name="sequence">2</field>
			<field name="view_id" ref="view_vhr_import_status_key_student" />
			<field name="act_window_id" ref="action_vhr_import_status_key_student" />
			<field name="view_mode">form</field>
		</record>

		<!-- Import talent -->
		<record id="view_vhr_import_status_talent" model="ir.ui.view">
			<field name="name">vhr.import.status.talent.form</field>
			<field name="model">vhr.import.status</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Import Talent">
					<div class="oe_title">
						<h2>
							<label string="Import Talent" />
						</h2>
					</div>
					<br />
					<group>
						<field name="db_datas" filename="name_data" />
						<field name="name_data" readonly="1" options="{'save_readonly' : True}" />
					</group>
					<div>
						<![CDATA[
							<a href="/vhr_recruitment/static/template/talent_template.xlsx">Talent template</a>
						]]>
					</div>
					<footer>
						<button name="import_excel_thread" string="Import"
							context="{'function': 'thread_import_talent',
										  'model': 'vhr.rr.talent',
										  'module':'module_vhr_recruitment'}"
							type="object" icon="gtk-apply"
							default_focus="1" />
						or
						<button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="action_vhr_import_status_talent" model="ir.actions.act_window">
			<field name="name">Import talent</field>
			<field name="res_model">vhr.import.status</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>
		<record id="action_vhr_import_talent_form" model="ir.actions.act_window.view">
			<field name="sequence">2</field>
			<field name="view_id" ref="view_vhr_import_status_talent" />
			<field name="act_window_id" ref="action_vhr_import_status_talent" />
			<field name="view_mode">form</field>
		</record>

		<!-- Import email subcribes -->
		<record id="view_vhr_import_email_subcribes" model="ir.ui.view">
			<field name="name">vhr.import.status.email.subcribes.form</field>
			<field name="model">vhr.import.status</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Import Talent">
					<div class="oe_title">
						<h2>
							<label string="Import Email Marketing" />
						</h2>
					</div>
					<br />
					<group>
						<field name="db_datas" filename="name_data" />
						<field name="name_data" readonly="1" options="{'save_readonly' : True}" />
					</group>
					<div>
						<![CDATA[
							<a href="/vhr_recruitment/static/template/email_marketing_template.xlsx">Email Marketing template</a>
						]]>
					</div>
					<footer>
						<button name="import_excel_thread" string="Import"
							context="{'function': 'thread_import_email_subcribes',
										  'model': 'vhr.email.news',
										  'module':'module_vhr_recruitment'}"
							type="object" icon="gtk-apply"
							default_focus="1" />
						or
						<button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>

		<record id="action_vhr_import_email_subcribes" model="ir.actions.act_window">
			<field name="name">Import Email Marketing</field>
			<field name="res_model">vhr.import.status</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>
		<record id="action_vhr_import_email_subcribes_form" model="ir.actions.act_window.view">
			<field name="sequence">2</field>
			<field name="view_id" ref="view_vhr_import_email_subcribes" />
			<field name="act_window_id" ref="action_vhr_import_email_subcribes" />
			<field name="view_mode">form</field>
		</record>
		
		<!-- Import result survey -->
		<record id="view_vhr_import_status_rr_survey" model="ir.ui.view">
			<field name="name">vhr.import.status.rr.survey.form</field>
			<field name="model">vhr.import.status</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Import RR Survey">
					<div class="oe_title">
						<h1>
							<label string="Import RR Survey" />
						</h1>
					</div>
					<br />
					<group>
						<field name="db_datas" filename="name_data" />
						<field name="name_data" readonly="1" options="{'save_readonly' : True}" />
					</group>
					<div>
						<![CDATA[
							<a href="/vhr_recruitment/static/template/Recruitment_Service_Survey_Responses.xlsx">Service Survey Responses</a>
						]]>
					</div>
					<footer>
						<button name="import_excel_thread" string="Import"
							context="{'function': 'thread_import_rr_survey',
										  'model': 'vhr.recruitment.temp.survey',
										  'module':'module_vhr_recruitment'}"
							type="object" icon="gtk-apply"
							default_focus="1" />
						or
						<button string="Cancel" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>
		
		<record id="action_vhr_import_status_rr_survey" model="ir.actions.act_window">
			<field name="name">Import Survey Responses</field>
			<field name="res_model">vhr.import.status</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>
		<record id="action_vhr_import_status_rr_survey_form" model="ir.actions.act_window.view">
			<field name="sequence">2</field>
			<field name="view_id" ref="view_vhr_import_status_rr_survey" />
			<field name="act_window_id" ref="action_vhr_import_status_rr_survey" />
			<field name="view_mode">form</field>
		</record>
		
	</data>
</openerp>