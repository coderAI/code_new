<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate='0'>
		<!--CV Online -->
		<record id="view_vhr_temp_applicant_cv_form" model="ir.ui.view">
			<field name="name">online.candidate.form</field>
			<field name="model">vhr.temp.applicant</field>
			<field name="arch" type="xml">
				<form string="CV Online" version="7.0">
					<header>
						<field name="state" widget="statusbar" statusbar_visible="draft,approved,reject" />
					</header>
					<sheet>
						<button name="action_reject_temp_applicant" class="oe_stat_button oe_right bg_icon_red"
							icon="fa-times" type="object" string="Reject"
							attrs="{'invisible': [('applicant_id', '!=', False)]}" />

						<button name="action_approve_temp_applicant" class="oe_stat_button oe_right bg_icon_green"
							icon="fa-mail-forward" type="object" context="{'type': 'candidate'}"
							string="Approved" attrs="{'invisible': [('applicant_id', '!=', False)]}" />
						<button name="action_get_attachment_tree_view"
							attrs="{'invisible':  [('attachment_number','=', 0)]}" class="oe_stat_button oe_right"
							icon="fa-book" type="object">
							<field name="attachment_number" widget="statinfo" string="Documents" />
						</button>
						<group string="General Information">
							<group>
								<field name="last_name" required='1' string="Last &amp; mid name*" />
								<field name="first_name" required='1' string="First name*" />
								<field name="email" string="Email*" required='1' widget="email"
									options="{'validate_email': True}" />
								<field name="birthday" string="Date of birth*" required='1'
									on_change="onchange_date(birthday)" />
							</group>
							<group>
								<field name="gender" required='1' string="Gender*" />
								<field name="mobile_phone" string="Mobile Phone*"
									required='1' options="{'validate_number': True}" />
								<field name="country_id" string="Nationality*" required='1'
									options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
							</group>

						</group>
						<group>
							<field name="note" />
						</group>
						<group string="CV Online">
							<group>
								<field name="post_job_id" required='1' string="Job Apply*"
									options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
								<field name="applicant_id" readonly="1"
									options="{'no_quick_create' : True, 'no_create':True}" />
							</group>
							<group>
								<field name="program_event_id" required='1' string="Program Event*"
									domain="[('program_id.code', '=', 'CANDIDATE'), ('active', '=', True)]"
									context="{'program_code':'CANDIDATE'}"
									options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />

								<field name="applicant_suggest_id" readonly="1"
									options="{'no_quick_create' : True, 'no_create':True}" />
								<field name="recruitment_suggest_source_id" readonly="1"
									options="{'no_quick_create' : True, 'no_create':True}" />
							</group>
						</group>
						<group string="CV Index" colspan="4">
							<field name="index_content" nolabel="1" />
						</group>
					</sheet>
				</form>
			</field>
		</record>

		<record id="view_vhr_temp_applicant_cv_tree" model="ir.ui.view">
			<field name="name">vhr.temp.applicant.tree</field>
			<field name="model">vhr.temp.applicant</field>
			<field name="arch" type="xml">
				<tree string="CV Online" version="7.0">
					<field name="last_name" />
					<field name="first_name" />
					<field name="email" />
					<field name="birthday" />
					<field name="gender" />
					<field name="mobile_phone" />
					<field name="post_job_id" />
					<field name="state" />
					<field name="program_event_id" invisible="1" />
					<field name="create_date" />
					<field name="create_uid" />
				</tree>
			</field>
		</record>

		<record id="view_search_cvonline_filter" model="ir.ui.view">
			<field name="name">vhr.temp.applicant.search</field>
			<field name="model">vhr.temp.applicant</field>
			<field name="arch" type="xml">
				<search string="Search Candidate">
					<field name="name" string="Candidate Info"
						filter_domain="['|', '|', '|', '|', ('name', 'ilike', self), ('first_name','ilike',self), ('last_name','ilike',self), ('email','ilike',self), ('mobile_phone','ilike',self)]" />
					<field name="index_content" string="CV Content" />
					<field name="program_event_id" />
					<field name="note" />
					<field name="post_job_id" />
					<separator />
					<group expand="0" string="Group By">
						<filter name="group_by_post_job_id" string="Job Apply"
							context="{'group_by':'post_job_id'}" />
						<filter name="group_by_state" string="State" context="{'group_by':'state'}" />
						<filter name="group_by_create_date" string="Create Date"
							context="{'group_by':'create_date'}" />
						<filter name="group_by_create_uid" string="Create User"
							context="{'group_by':'create_uid'}" />
					</group>
					<group expand="0" string="Filter">
						<filter string="Draft" name="filter_draft" icon="STOCK_NEW"
							domain="[('state', '=', 'draft')]" />
						<filter string="Reject" name="filter_reject" icon="STOCK_NEW"
							domain="[('state', '=', 'reject')]" />
						<filter string="Approved" name="filter_approved" icon="STOCK_NEW"
							domain="[('state', '=', 'approved')]"/>
					</group>
				</search>
			</field>
		</record>

		<record model="ir.actions.act_window" id="action_vhr_temp_applicant_cv">
			<field name="name">Online Candidates</field>
			<field name="res_model">vhr.temp.applicant</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain"
				eval="[('program_event_id.program_id', '=', ref('vhr_recruitment.hrs_data_program_candidate'))]" />
			<field name="search_view_id" ref="view_search_cvonline_filter" />
			<field name="context">{"search_default_filter_draft":1,
				'PROGRAM':'CANDIDATE','DIMENSION_TYPE':
				'DND_PROGRAM',"active_test":False}
			</field>
			<field name="groups_id" eval="" />
		</record>

		<record id="action_view_vhr_temp_applicant_cv_tree" model="ir.actions.act_window.view">
			<field name="sequence">7</field>
			<field name="view_id" ref="view_vhr_temp_applicant_cv_tree" />
			<field name="act_window_id" ref="action_vhr_temp_applicant_cv" />
			<field name="view_mode">tree</field>
		</record>

		<record id="action_view_vhr_temp_applicant_cv_form" model="ir.actions.act_window.view">
			<field name="sequence">8</field>
			<field name="view_id" ref="view_vhr_temp_applicant_cv_form" />
			<field name="act_window_id" ref="action_vhr_temp_applicant_cv" />
			<field name="view_mode">form</field>
		</record>

