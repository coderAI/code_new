							context="{'group_by':'job_id'}" />
						<filter name="group_by_program_event_id" string="Program Event"
							context="{'group_by':'program_event_id'}" />
						<filter name="group_by_gender" string="Gender"
							context="{'group_by':'gender'}" />
						<filter name="group_by_create_date" string="Create Date"
							context="{'group_by':'create_date'}" />
						<filter name="group_by_create_uid" string="Create User"
							context="{'group_by':'create_uid'}" />
					</group>
					<group expand="0" string="Filter">
						<filter string="Draft" name="filter_draft" icon="STOCK_NEW"
							domain="[('state', '=', 'draft')]" />
						<filter string="Reject" name="filter_reject" icon="STOCK_NEW"
							domain="[('state', '=', 'reject')]" />
						<filter string="Approved" name="filter_approved" icon="STOCK_NEW"
							domain="[('state', '=', 'approved')]" />
					</group>
