		<!-- INTERNSHIP -->

		<record id="view_vhr_temp_applicant_internship_form" model="ir.ui.view">
			<field name="name">vhr.temp.applicant.form</field>
			<field name="model">vhr.temp.applicant</field>
			<field name="arch" type="xml">
				<form string="Internship" version="7.0">
					<header>
						<field name="state" widget="statusbar" statusbar_visible="draft,approved,reject" />
					</header>
					<sheet>
						<button name="action_reject_temp_applicant" class="oe_stat_button oe_right bg_icon_red"
							icon="fa-times" type="object" string="Reject"
							attrs="{'invisible': [('applicant_id', '!=', False)]}" />
						<button name="action_approve_temp_applicant" class="oe_stat_button oe_right bg_icon_green"
							icon="fa-mail-forward" type="object" string="Approved"
							attrs="{'invisible': [('applicant_id', '!=', False)]}" context="{'type': 'internship'}" />
						<button name="action_get_attachment_tree_view"
							attrs="{'invisible':  [('attachment_number','=', 0)]}" class="oe_stat_button oe_right"
							icon="fa-book" type="object">
							<field name="attachment_number" widget="statinfo" string="Documents" />
						</button>




						<group string="General Information">
							<group>
								<field name="last_name" required='1' string="Last  &amp; mid Name*" />
								<field name="first_name" required='1' string=" First Name*" />
								<field name="birthday" required='1' on_change="onchange_date(birthday)"
									string="Date of birth*" />
								<field name="gender" required='1' />

								<field name="identification_no" options="{'validate_number': True}" />
							</group>
							<group>
								<field name="email" required='1' widget="email"
									options="{'validate_email': True}" string="Email*" />
								<field name="mobile_phone" required='1'
									options="{'validate_number': True}" string="Mobile Phone*" />
								<field name="permanent_address" />
							</group>
						</group>
						<notebook>

							<page string="Education Information">
								<group string="Education Information">
									<group>
										<field name="school_id" required='1' string="University*"
											options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
										<field name="recruitment_degree_id" required='1' string="Degree*"
											options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
										<field name="cumulative_gpa" required='1' string="Cumulative GPA*" />
										<field name="recruitment_source_id" required='1'
											options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
										<field name="recruitment_source_online_id" required='1'
											options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
										<field name="applicant_id" readonly="1"
											options="{'no_quick_create' : True, 'no_create':True}" />
									</group>
									<group>
										<!-- <field name="speciality" required='1' string="Speciality*" 
											/> -->
										<field name="speciality_id" required='1' string="Speciality*"
											options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
										<field name="time_graduation_id" required='1'
											string="Time Graduation*" context="{'dimension_type':'TIME_GRADUATION'}"
											options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
										<field name="note" />
									</group>
								</group>
								<group string="Intern Time">
									<group>
										<field name="program_event_id" required='1' string="Program Event*"
											domain="[('program_id.code', '=', 'INTERNSHIP'), ('active', '=', True)]"
											context="{'program_code':'INTERNSHIP'}"
											options="{'no_quick_create' : True, 'no_create':True, 'no_open': True}" />
									</group>
								</group>
							</page>
							<page string="Test result">
								<field name="test_result_ids" />
							</page>
						</notebook>
					</sheet>
				</form>
			</field>
		</record>

		<record id="view_vhr_temp_applicant_internship_tree" model="ir.ui.view">
			<field name="name">vhr.temp.applicant.tree</field>
			<field name="model">vhr.temp.applicant</field>
			<field name="arch" type="xml">
