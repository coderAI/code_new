<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_vhr_dimension_form" model="ir.ui.view">
            <field name="name">vhr.dimension.view.form</field>
            <field name="model">vhr.dimension</field>
            <field name="groups_id" eval=""/>
            <field name="arch" type="xml">
                <form string="Dimension" version="7.0">
       				<sheet>
       					<group col="4">
       						<group>
       							<field name="name" required="1"/>
       							<field name="code" required="1"/>
       							<field name="level" 
       							 attrs="{'invisible': [('is_show_level','=',False)]}"/>
       						</group>
       						<group>
       							<field name="name_en"/>
	       						<field name="dimension_type_id" required="1" readonly="1"
	       							on_change="onchange_dimension_type_id(dimension_type_id)"
	       							context = "{'code': context.get('dimension_type', False)}"/>
	                            <field name="active"/>
	                            <field name="is_show_level" invisible='1'/>
       						</group>
       					</group>
       					<group>
       						<field name="description" string="Description"/>
       					</group>
       					<notebook>
       						<page string="Logs">
       							<label string="*** Please press F5 to refresh the list of logs !" colspan="4"/>	
       							<field name="audit_log_ids" mode="tree"
                       		 		widget="one2many_list" readonly="1">
	       							<tree version="7.0">
	       								<field name="timestamp" required="1" readonly="1"/>
				                        <field name="user_id" readonly="1"/>
				                        <field name="method" readonly="1"/>
				                        <field name="field_description"/>
                            			<field name="old_value_text"/>
                            			<field name="new_value_text"/>
	       							</tree>
	       						</field>	
       						</page>	
       					</notebook>		
       				</sheet>
       			</form>
            </field>
        </record>

        <record id="view_vhr_dimension_tree" model="ir.ui.view">
            <field name="name">vhr.dimension.view.tree</field>
            <field name="model">vhr.dimension</field>
            <field name="groups_id" eval=""/>
            <field name="arch" type="xml">
                <tree string="Dimension" version="7.0">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="name_en"/>
<!--                    <field name="dimension_type_id"/-->
<!--                    <field name="level"/-->
                    <field name="active"/>
                </tree>
            </field>
        </record>

        <record id="view_vhr_dimension_search" model="ir.ui.view">
            <field name="name">vhr.dimension.view.search</field>
            <field name="model">vhr.dimension</field>
            <field name="arch" type="xml">
                <search string="Search">
                	<field name="name" string="Dimension" 
        			 filter_domain="['|','|',('name','ilike',self),('name_en','ilike',self),('code','ilike',self)]"/>
        			
                    <filter string="Active = True" name="active_true" icon="terp-stage"
                            domain="[('active', '=', True)]"/>
                    <filter string="Active = False" name="active_false" icon="STOCK_NEW"
                            domain="[('active', '=', False)]"/>
                </search>
            </field>
        </record>

        <record model="ir.actions.act_window" id="act_vhr_dimension">
