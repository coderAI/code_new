            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="view_id" ref="view_vhr_dimension_tree"/>
            <field name="groups_id" eval=""/>
            <field name="search_view_id" ref="view_vhr_dimension_search"/>
        </record>
       
